{
  "private": true,
  "name": "@rdf-toolkit/explorer-site-generator",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "./dist/**/*"
  ],
  "main": "./dist/explorer-site-generator.js",
  "bin": {
    "rdf-explorer-site-generator": "./dist/explorer-site-generator.js"
  },
  "eslintIgnore": [
    "src/scripts/explorer.asset.js"
  ],
  "scripts": {
    "lint": "npx eslint src/",
    "prebuild": "tsc --noEmit",
    "build": "esbuild src/main.tsx --bundle --minify --platform=node --format=esm --target=es2020 --asset-names=assets/[hash] --loader:.asset.js=file --loader:.woff2=file --outfile=./dist/explorer-site-generator.js",
    "clean": "rimraf ./dist/explorer-site-generator.{js,css} ./dist/assets/*.{js,woff2}"
  },
  "devDependencies": {
    "@rdf-toolkit/iterable": "^0.1.0",
    "@rdf-toolkit/rdf": "^0.1.0",
    "@rdf-toolkit/schema": "^0.1.0",
    "@rdf-toolkit/text": "^0.1.0",
    "@rdf-toolkit/turtle": "^0.1.0",
    "@types/commonmark": "^0.27.5",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "commonmark": "^0.30.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.34.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5"
  }
}
